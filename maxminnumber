public class Solution
{
  public static void main(String[] args) 
  {
    int arr[] = {1234,222,-23569,3652,32569,2458,63258,-96582};		 
   
    int max = arr[0];
    int min = arr[0];
    
    for(int i = 1; i < arr.length; i++)
    {
      if(max < arr[i])
      {
        max = arr[i];
      } else if(min > arr[i])
      {
        min = arr[i];
      }		   		   
    }
    System.out.println("Maximum number = "  + max +  " Minimum number = " + min);		
  }
}
